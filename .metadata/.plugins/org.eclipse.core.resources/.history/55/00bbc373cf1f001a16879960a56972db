#include <iostream>
#include "Stack.h"

template <class T>
Stack<T>::Stack() {
	top = NULL;
}

template <class T>
void Stack<T>::push(T num){
	if(top==NULL){
		std::cout<< "stack Overflow";
		return;
	}
	top = top->next;   //top++;
	data[top] = num;
}

template <class T>
T Stack<T>::pop(){
	if(top>0){
		top--;
		return data[top+1];
	}
	std::cout << "stack Empty";
	return -1;
}

template <class T>
Stack<T>::~Stack() {

}

