#include <iostream>

using namespace std;
#include "stack.h"

template <class T>
stack<T>::stack() {
	top = 0;
}

template <class T>
void stack<T>::push(T num){
	if(top>=MAX){
		cout<< "stack Overflow";
		return;
	}
	top++;
	data[top] = num;
}

template <class T>
T stack<T>::pop(){
	if(top>0){
		top--;
		return data[top+1];
	}
	cout << "stack Empty";
	return -1;
}

template <class T>
stack<T>::~stack() {
}

