#include <iostream>
#include "Stack.h"

template <class T>
Stack<T>::Stack() {
	top = NULL;
}

template <class T>
void Stack<T>::push(T num){
	Node<T> * p;
	p = new Node<T>;
	top->next = p;
	top = top->next;   //top++;
	top->data = num;	//data[top] = num;
}

template <class T>
T Stack<T>::pop(){
	T temp;
	temp = top->data;
	std::cout << "stack Empty";
	return -1;
}

template <class T>
Stack<T>::~Stack() {

}

