#include <iostream>
using namespace std;
#include "stack.h"

stack::stack() {
	top = 0;
}

void stack::push(int num){
	if(top>=MAX){
		cout<< "stack Overflow";
		return;
	}
	top++;
	data[top] = num;
}

int stack::pop(){
	if(top>0){
		top--;
		return data[top+1];
	}
	cout << "stack Empty";
	return -1;
}

stack::~stack() {

}

